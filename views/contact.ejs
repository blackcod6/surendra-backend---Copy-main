        <!-- login form start  -->
    <div class="login-form">
      <div class="container">
        <div class="card">
          <div class="inner-box" id="card">
            <div class="card-front">
              <h1>SIGN UP</h1>
              <form action="/signup" method="post" >
                <img id="name" src="/name.png" alt="" />
                <input
                  type="text"
                  class="input-box"
                  placeholder="Your Name"
                  name="myname"
                  id="idname"
                  oninput="validateName(this)"
                  maxlength="32"
                  required
                />              


                <img id="email" src="/mail-outline.svg" alt="" />
                <input
                  type="email"
                  class="input-box"
                  placeholder="Email"
                  name="myemail"
                  id="idEmail"
                  onkeyup="valiemail()"
                  required
                />
                <img id="pass1" src="/lock-closed-outline.svg" alt="" />
                
                <input
                  type="password"
                  class="input-box"
                  placeholder="Password"
                  name="mypassword"
                  id="idpassword"
                  required
                />
                <img id="pass2" src="/lock-closed-outline.svg" alt="" />
                <input
                  type="password"
                  class="input-box"
                  placeholder="Confirm Password"
                  name="myconfirmpassword"
                  id="idconfirmpassword"
                  required
                />
                <label id="rem">
                  <input class="contact-field" type="checkbox" /><span>Remember Me</span>
                </label>
                <a href="#" id="forgot">Forgot Password</a>
                <button type="submit" class="sumbit-btn" onclick=" return validata()">Sumbit</button>
                <span id="accountregister">Don't have a account </span>
                <button type="button" class="btn" onclick="openlogin()" >
                  Log In
                </button>
              </form>
            </div>
            <div class="card-back">
              <h1>LOGIN</h1>
              <form action="/account"   method="post">
                <img id="email" src="/mail-outline.svg" alt="" />
                <input
                  type="email"
                  class="input-box"
                  placeholder="Email"
                  name="myloginemail"
                  required
                />
                <img id="pass1" src="/lock-closed-outline.svg" alt="" />
                <input
                  type="password"
                  class="input-box"
                  placeholder="Password"
                  name="myloginpass"
                  required
                />
                <label id="remback">
                  <input type="checkbox" /><span>Remember Me</span>
                </label>
                <a href="#" id="forgot">Forgot Password</a>
                <button type="submit" class="sumbit-btn">Sumbit</button>
                <span id="accountregister">Already have a account </span>
                <button type="button" onclick="openregister()" class="btn">
                  Register Now
                </button>
              </form>
            </div>
          </div>
        </div>
      </div>
    </div>

    <!-- login form end  -->
























       <!-- contact-us start-->
       <div id="Aayush">
    <div class="contactcontainer">
        <h1>Contact Us</h1>
        <p>
          We would love to respond to your queries and help you succeed. Feel free
          to get in touch with us
        </p>
      </div>
      <div class="contact-box">
        <div class="contact-left">
          <h3>Sent your request</h3>
          <form>
            <div class="input-row">
              <div class="input-group">
                <label>Name</label>
                <input  class="contact-field1" type="text" placeholder="Enter your Name" />
              </div>
              <div class="input-group">
                <label>Phone</label>
                <input class="contact-field1" type="number" placeholder="123xxxxxx90" />
              </div>
            </div>
            <div class="input-row">
              <div class="input-group">
                <label id="Email">Email</label>
                <input class="contact-field1" type="email" placeholder="abc@gmail.com" />
              </div>
              <div class="input-group">
                <label>Subject</label>
                <input class="contact-field1" type="text" placeholder="product demo" />
              </div>
            </div>
            <div>
              <label>Message</label>
              <textarea rows="5" placeholder="Your Message"></textarea>
            </div>
            <div id="contactcontainer_button">
              <button type="submit">SEND</button>
            </div>
          </form>
        </div>
      </div>
    </div>
  
